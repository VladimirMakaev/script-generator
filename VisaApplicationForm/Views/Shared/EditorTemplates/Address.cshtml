@model VisaApplicationForm.Models.Address

<ol>
    <li>
        @Html.LabelFor(m => m.Country)
        @Html.TextBoxFor(m => m.Country)
    </li>
    <li>
        @Html.LabelFor(m => m.State)
        @Html.TextBoxFor(m => m.State)
    </li>
    <li>
        @Html.LabelFor(m => m.City)
        @Html.TextBoxFor(m => m.City)
    </li>
    <li>
        @Html.LabelFor(m => m.AddressLine1)
        @Html.EditorFor(m => m.AddressLine1)
    </li>
    <li>
        @Html.LabelFor(m => m.PostalCode)
        @Html.TextBoxFor(m => m.PostalCode)
    </li>
    <li>
        @Html.LabelFor(m => m.PhonePrefix)
        @Html.TextBoxFor(m => m.PhonePrefix)
    </li>
    <li>
        @Html.LabelFor(m => m.PhoneNumber)
        @Html.TextBoxFor(m => m.PhoneNumber)
    </li>
    <li>
        @Html.LabelFor(m => m.Email)
        @Html.TextBoxFor(m => m.Email)
    </li>
</ol>
